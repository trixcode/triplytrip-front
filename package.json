    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.4",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "2.50.1",
